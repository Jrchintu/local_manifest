<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote  name="chintuos" fetch="https://github.com/chintuos" />
	
        # Frameworks base
	<remove-project name="platform_frameworks_base" />
	<project path="frameworks/base" name="platform_frameworks_base" remote="chintuos" revision="auto"/>

        # Telephony
	<remove-project name="platform_frameworks_opt_telephony" />
	<project path="frameworks/opt/telephony" name="platform_frameworks_opt_telephony" remote="chintuos" revision="auto"/>
	
        # Telophony-2
        <remove-project name="android_vendor_codeaurora_telephony" />
	<project path="vendor/codeaurora/telephony" name="StagOS/android_vendor_codeaurora_telephony" remote="github" revision="r11"/>
	
	# Settings
	<remove-project name="platform_packages_apps_Settings" />
	<project path="packages/apps/Settings" name="platform_packages_apps_Settings" remote="chintuos" revision="auto"/>
	
</manifest>
